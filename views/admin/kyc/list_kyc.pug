extends ../../layoutadmin
block content_dashboard
  .row
    .col-12
      .card
        .card-body
          .form-group.row
            label.col-lg-2.col-form-label(for='val-username')
                h4.card-title All User Kyc
          .form-group.row
            input.btn.btn-primary.divcenter(type='submit', value='Check User Kyc Random',onclick='checkrandom();')
          .table-responsive
            table#example23.display.nowrap.table.table-hover.table-striped.table-bordered(cellspacing='0', width='100%')
              thead
                tr
                  th STT
                  th UserName
                  th Phone
                  th Status
                  th National
                  th Date KYC
                  th Action
              tbody
                
                
